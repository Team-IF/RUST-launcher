{
  "name": "rust-launcher",
  "version": "0.1.0",
  "description": "Minecraft modpack launcher for RUST server",
  "repository": "https://github.com/Team-IF/RUST-Launcher",
  "author": "Team-IF",
  "license": "AGPL-3.0-or-later",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "testbuild": "electron-builder --dir",
    "build": "electron-builder"
  },
  "build": {
    "appId": "team.if.rust-launcher",
    "productName": "RUST Launcher",
    "win": {
      "icon": "img/icon.ico"
    },
    "nsis": {
      "oneClick": true,
      "installerIcon": "img/icon.ico",
      "uninstallerIcon": "img/icon.ico"
    }
  },
  "dependencies": {
    "@xmcl/core": "^2.0.2",
    "axios": "^0.19.2",
    "minecraft-launcher-core": "^3.12.0",
    "node-machine-id": "^1.1.12",
    "open": "^7.0.2"
  },
  "devDependencies": {
    "electron": "^9.4.0",
    "electron-builder": "^22.3.2"
  }
}